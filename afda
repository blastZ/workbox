[32m+[m				[32mCOLUMN_ALARM_NAME[m
[32m+[m				[32m};[m
[32m+[m		[32mreturn getDatabase().query(ALARM_TABLE, columns, null, null, null, null,[m
[32m+[m				[32mnull);[m
[32m+[m	[32m}[m
