[32m+[m[32m                MainActivity.this.startActivity(intent);[m
[32m+[m[32m            }else if(position==6){[m
[32m+[m[32m                intent=new Intent(MainActivity.this,Calculator.class);[m
[32m+[m[32m                MainActivity.this.startActivity(intent);[m
[32m+[m[32m            }else if(position==7){[m
