[32m+[m[32m            }else if(position==9){[m
[32m+[m[32m                intent=new Intent(MainActivity.this,AlarmActivity.class);[m
[32m+[m[32m                MainActivity.this.startActivity(intent);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
