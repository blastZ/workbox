[32m+[m						[32m@Override[m
[32m+[m						[32mpublic void onCancel(DialogInterface dialog) {[m
[32m+[m							[32malarmPreferenceListAdapter.setMathAlarm(getMathAlarm());[m
[32m+[m							[32malarmPreferenceListAdapter.notifyDataSetChanged();[m
