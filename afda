[32m+[m											[32malarmToneTimer.cancel();[m
[32m+[m										[32malarmToneTimer = new CountDownTimer(3000, 3000) {[m
[32m+[m											[32m@Override[m
[32m+[m											[32mpublic void onTick(long millisUntilFinished) {[m
[32m+[m
[32m+[m											[32m}[m
